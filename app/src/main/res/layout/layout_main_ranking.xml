<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="100dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="8dp">
                <TextView
                    android:text="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_TextView_Ranking1"
                    android:textSize="13dp"
                    android:textColor="@color/Black"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp" />

                <ImageView
                    app:srcCompat="@drawable/agree"
                    android:id="@+id/Main_Ranking_ImageView_Emblem1"
                    android:scaleType="fitXY"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:text="마동쿠스브라디서"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_ImageView_TeamName1"
                    android:layout_weight="4"
                    android:textColor="@color/Black"
                    android:gravity="bottom|center"
                    android:textSize="10dp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="8dp">
                <TextView
                    android:text="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_TextView_Ranking2"
                    android:textSize="13dp"
                    android:textColor="@color/Black"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp" />

                <ImageView
                    app:srcCompat="@drawable/agree"
                    android:id="@+id/Main_Ranking_ImageView_Emblem2"
                    android:scaleType="fitXY"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:text="마동쿠스브라디서"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_ImageView_TeamName2"
                    android:layout_weight="4"
                    android:textColor="@color/Black"
                    android:gravity="bottom|center"
                    android:textSize="10dp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="8dp">
                <TextView
                    android:text="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_TextView_Ranking3"
                    android:textSize="13dp"
                    android:textColor="@color/Black"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp" />

                <ImageView
                    app:srcCompat="@drawable/agree"
                    android:id="@+id/Main_Ranking_ImageView_Emblem3"
                    android:scaleType="fitXY"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:text="마동쿠스브라디서"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_ImageView_TeamName3"
                    android:layout_weight="4"
                    android:textColor="@color/Black"
                    android:gravity="bottom|center"
                    android:textSize="10dp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="8dp">
                <TextView
                    android:text="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/Main_Ranking_TextView_Ranking4"
                    android:textSize="13dp"
                    android:textColor="@color/Black"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp" />

                <ImageView
                    app:srcCompat="@drawable/agree"
                    android:id="@+id/Main_Ranking_ImageView_Emblem4"
